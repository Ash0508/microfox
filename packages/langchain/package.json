{
  "name": "@microfox/langchain",
  "version": "1.0.1",
  "description": "Microfox adapter for the LangChain AI SDK.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microfox-ai/microfox.git"
  },
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prettier-check": "prettier --check \"./**/*.ts*\""
  },
  "dependencies": {
    "@microfox/core": "*"
  },
  "peerDependencies": {
    "@langchain/core": "^0.3.42"
  },
  "devDependencies": {
    "@microfox/tsconfig": "*",
    "@langchain/core": "^0.3.42"
  },
  "publishConfig": {
    "access": "public"
  }
}
