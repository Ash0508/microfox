{
  "name": "@microfox/mastra",
  "version": "7.5.1",
  "description": "Microfox adapter for the Mastra AI Agent SDK.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microfox-ai/microfox.git"
  },
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del dist",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@microfox/core": "*"
  },
  "peerDependencies": {
    "@mastra/core": "^0.6.1"
  },
  "devDependencies": {
    "@microfox/tsconfig": "*",
    "@mastra/core": "^0.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
